<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE interface>
<interface>
  <requires lib="gtk" version="4.0" />
  <object class="GtkApplicationWindow" id="window">
    <property name="title">Climate Tracker</property>
    <property name="default-width">320</property>
    <property name="default-height">200</property>
    <child>
      <object class="GtkGrid">
        <property name="column-spacing">5</property>
        <property name="row-spacing">5</property>
        <property name="halign">GTK_ALIGN_CENTER</property>
        <property name="valign">GTK_ALIGN_CENTER</property>
        <child>
          <object class="GtkButton" id="button-1">
            <property name="label">Button 1</property>
            <signal name="clicked" handler="print_hello" />
            <layout>
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="button-2">
            <property name="label">Button 2</property>
            <signal name="clicked" handler="print_hello" />
            <layout>
              <property name="column">1</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="button-3">
            <property name="label">Button 3</property>
            <signal name="clicked" handler="print_hello" />
            <layout>
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="button-4">
            <property name="label">Button 4</property>
            <signal name="clicked" handler="print_hello" />
            <layout>
              <property name="column">1</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
